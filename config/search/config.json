{
  "batch_size": 128,
  "max_batches": 0,
  "attachments_path_base": "/data/",
  "eager_indexing_sparql_query": false,
  "additive_indexes": true,
  "persist_indexes": true,
  "update_wait_interval_minutes": 0,
  "automatic_index_updates": true,
  "eager_indexing_groups": [
    [
      {
        "name": "clean",
        "variables": []
      }
    ],
    [
      {
        "name": "public",
        "variables": []
      }
    ]
  ],
  "default_settings": {
    "analysis": {
      "analyzer": {
        "dutchanalyzer": {
          "tokenizer": "standard",
          "filter": ["lowercase", "asciifolding", "dutchstemmer"]
        }
      },
      "filter": {
        "dutchstemmer": {
          "type": "stemmer",
          "name": "dutch"
        }
      }
    }
  },
  "types": [
    {
      "type": "agendapunt",
      "on_path": "agendapunten",
      "rdf_type": "http://data.vlaanderen.be/ns/besluit#Agendapunt",
      "properties": {
        "title": "http://purl.org/dc/terms/title",
        "description": "http://purl.org/dc/terms/description",
        "plannedPublic": "http://data.vlaanderen.be/ns/besluit#geplandOpenbaar",
        "references": "http://purl.org/dc/terms/references",

        "typeURI": "http://data.vlaanderen.be/ns/besluit#Agendapunt.type",
        "type": [
          "http://data.vlaanderen.be/ns/besluit#Agendapunt.type",
          "http://www.w3.org/2004/02/skos/core#prefLabel"
        ],
        "draftResolutionURIs": "http://data.vlaanderen.be/ns/besluit#heeftOntwerpbesluit",

        "zittingURI": "^http://data.vlaanderen.be/ns/besluit#behandelt",
        "zittingPlannedStart": [
          "^http://data.vlaanderen.be/ns/besluit#behandelt",
          "http://data.vlaanderen.be/ns/besluit#geplandeStart"
        ],
        "zittingStartedAtTime": [
          "^http://data.vlaanderen.be/ns/besluit#behandelt",
          "http://www.w3.org/ns/prov#startedAtTime"
        ],
        "zittingVerslagURI": [
          "^http://data.vlaanderen.be/ns/besluit#behandelt",
          "http://data.vlaanderen.be/ns/besluit#heeftZittingsverslag"
        ],
        "zittingNotulenURI": [
          "^http://data.vlaanderen.be/ns/besluit#behandelt",
          "http://data.vlaanderen.be/ns/besluit#heeftNotulen"
        ],

        "governingBodyURI": [
          "^http://data.vlaanderen.be/ns/besluit#behandelt",
          "http://data.vlaanderen.be/ns/besluit#isGehoudenDoor"
        ],
        "governingBody": [
          "^http://data.vlaanderen.be/ns/besluit#behandelt",
          "http://data.vlaanderen.be/ns/besluit#isGehoudenDoor",
          "http://www.w3.org/2004/02/skos/core#prefLabel"
        ],
        "governingBodyClassificationURI": [
          "^http://data.vlaanderen.be/ns/besluit#behandelt",
          "http://data.vlaanderen.be/ns/besluit#isGehoudenDoor",
          "http://data.vlaanderen.be/ns/besluit#classificatie"
        ],
        "governingBodyClassification": [
          "^http://data.vlaanderen.be/ns/besluit#behandelt",
          "http://data.vlaanderen.be/ns/besluit#isGehoudenDoor",
          "http://data.vlaanderen.be/ns/besluit#classificatie",
          "http://www.w3.org/2004/02/skos/core#prefLabel"
        ],

        "administrativeUnitURI": [
          "^http://data.vlaanderen.be/ns/besluit#behandelt",
          "http://data.vlaanderen.be/ns/besluit#isGehoudenDoor",
          "http://data.vlaanderen.be/ns/besluit#bestuurt"
        ],
        "administrativeUnit": [
          "^http://data.vlaanderen.be/ns/besluit#behandelt",
          "http://data.vlaanderen.be/ns/besluit#isGehoudenDoor",
          "http://data.vlaanderen.be/ns/besluit#bestuurt",
          "http://www.w3.org/2004/02/skos/core#prefLabel"
        ],
        "administrativeUnitClassificationURI": [
          "^http://data.vlaanderen.be/ns/besluit#behandelt",
          "http://data.vlaanderen.be/ns/besluit#isGehoudenDoor",
          "http://data.vlaanderen.be/ns/besluit#bestuurt",
          "http://data.vlaanderen.be/ns/besluit#classificatie"
        ],
        "administrativeUnitClassification": [
          "^http://data.vlaanderen.be/ns/besluit#behandelt",
          "http://data.vlaanderen.be/ns/besluit#isGehoudenDoor",
          "http://data.vlaanderen.be/ns/besluit#bestuurt",
          "http://data.vlaanderen.be/ns/besluit#classificatie",
          "http://www.w3.org/2004/02/skos/core#prefLabel"
        ],

        "agendaItemHandlingURI": [
          "^http://purl.org/dc/terms/subject"
        ],
        "agendaItemHandlingIsPublic": [
          "^http://purl.org/dc/terms/subject",
          "http://data.vlaanderen.be/ns/besluit#openbaar"
        ],

        "generatedResolutionURIs": [
          "^http://purl.org/dc/terms/subject",
          "http://www.w3.org/ns/prov#generated"
        ],
        "generatedResolutionTitleShorts": [
          "^http://purl.org/dc/terms/subject",
          "http://www.w3.org/ns/prov#generated",
          "http://data.europa.eu/eli/ontology#title_short"
        ],
        "generatedResolutionDescriptions": [
          "^http://purl.org/dc/terms/subject",
          "http://www.w3.org/ns/prov#generated",
          "http://data.vlaanderen.be/ns/besluit#motivering"
        ],
        "generatedResolutionMotivations": [
          "^http://purl.org/dc/terms/subject",
          "http://www.w3.org/ns/prov#generated",
          "http://data.vlaanderen.be/ns/besluit#motivering"
        ],
        "generatedResolutionPublicationDates": [
          "^http://purl.org/dc/terms/subject",
          "http://www.w3.org/ns/prov#generated",
          "http://www.w3.org/2001/XMLSchema#date"
        ]
      },
      "mappings": {
        "properties": {
          "title": { "type": "text" },
          "description": { "type": "text" },
          "plannedPublic": { "type" : "boolean" },
  
          "typeURI": { "type": "keyword" },
          "type": { "type": "keyword" },

          "zittingPlannedStart": { "type": "date" },
          "zittingStartedAtTime": { "type": "date" },

          "governingBodyURI": { "type": "keyword" },
          "governingBody": {"type": "keyword"},
          "governingBodyClassificationURI": {"type": "keyword"},
          "governingBodyClassification": {"type": "keyword"},

          "administrativeUnitURI": {"type": "keyword"},
          "administrativeUnit": {"type": "keyword"},
          "administrativeUnitClassification": {"type": "keyword"},
          "administrativeUnitClassificationURI": {"type": "keyword"},

          "agendaItemHandlingIsPublic": { "type": "boolean" },

          "generatedResolutionTitleShort": { "type": "text" },
          "generatedResolutionDescription": { "type": "text" },
          "generatedResolutionMotivation": { "type": "text" },
          "generatedResolutionPublicationDate": { "type": "date" }
        }
      }
    }
  ]
}
